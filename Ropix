            function openNav() {
                document.getElementById("mySidenav").style.width = "220px";
                document.getElementById("mySidenav").style.padding = "40px";
                document.getElementById("mySidenav").style.left = "60px";
                document.getElementById("close").style.zIndex = "5";
                document.getElementById("op").style.zIndex = "0";
                document.getElementById("op").style.display = "None";
            }
            function closeNav() {
                document.getElementById("mySidenav").style.width = "0";
                document.getElementById("mySidenav").style.padding = "0px";
                document.getElementById("mySidenav").style.left = "0px";
                document.getElementById("close").style.zIndex = "0";
                document.getElementById("op").style.zIndex = "5";
                document.getElementById("op").style.display = "Block";
            }
        </script>

        
